<odoo>
    <data>
        <record id="insurance_policy_rule" model="ir.rule">
            <field name="name">Insurance Policy</field>
            <field name="model_id" ref="model_insurance_policy"/>
            <field name="domain_force">[('company', 'in', company_ids)]</field>
        </record>

        <record id="insurance_config_rule" model="ir.rule">
            <field name="name">Insurance Configuration</field>
            <field name="model_id" ref="model_insurance_config_settings"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>


    </data>
</odoo>