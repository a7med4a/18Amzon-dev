<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="ir_cron_auto_create_timesheets" model="ir.cron">
        <field name="name">Auto Create Limousine Timesheets</field>
        <field name="model_id" ref="model_limousine_contract"/>
        <field name="state">code</field>
        <field name="code">model._auto_create_daily_timesheets()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">True</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 08:00:00')"/>
    </record>

</odoo>
